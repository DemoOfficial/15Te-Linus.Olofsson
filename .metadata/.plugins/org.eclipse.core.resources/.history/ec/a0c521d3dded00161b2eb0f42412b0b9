	//börja spelet
	
	
	String Komando="Exit";
	System.out.print("Vad ska vi göra?");
	Scanner in = new Scanner(System.in);
	Komando = in.nextLine();
	//Håll på så länge det masika ordet INTE är korrekt/inmatat
	while (!(Komando.equals("Exit")  )  )
	{
		//här hanteras sakern som skall göras och hända?
		System.out.print("Vad ska vi göra?");
		Komando = in.nextLine();
	
	}
